Exit:

	Program exit
	System.exit(0)

Icon:
	@Override
	public Image getIconImage() {
		Image retValue = Toolkit.getDefaultToolkit().getImage(ClassLoader.getSystemResource(patch));
		return retValue;
	} 
	
